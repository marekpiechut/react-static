{
  "name": "react-static",
  "description": "A progressive static site generator for React",
  "version": "7.6.2",
  "license": "MIT",
  "repository": "https://github.com/react-static/react-static",
  "main": "lib/index.js",
  "types": "src/index.d.ts",
  "engines": {
    "node": ">=8.9.0"
  },
  "bin": {
    "react-static": "./bin/react-static"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "cross-env REACT_STATIC_INTERNAL=true babel src --out-dir lib --ignore \"**/__tests__/*\" --ignore \"**/__mocks__/*\"  --source-maps inline",
    "watch": "yarn build --watch",
    "watch:link": "onchange './lib/**/*' -- yalc push --changed",
    "start": "yarn watch",
    "test": "yarn lint && yarn unitTest",
    "lint": "eslint src",
    "format": "prettier README.md {src,templates/*/src}/**/*.js --write",
    "unitTest": "cross-env NODE_ENV=test yarn jest ./src",
    "unitTestWatch": "cross-env NODE_ENV=test yarn jest src --watch",
    "preversion": "yarn build && yarn test",
    "publishLink": "echo '{{event}} to {{changed}}' && yalc publish"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.1"
  },
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-optional-chaining": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.20.7",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.20.7",
    "@reach/router": "^1.3.4",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-universal-import": "^4.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "circular-dependency-plugin": "^5.2.2",
    "cors": "^2.8.5",
    "css-loader": "^6.7.3",
    "download-git-repo": "^3.0.2",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "file-loader": "6.2.0",
    "fs-extra": "^11.1.0",
    "git-promise": "^1.0.0",
    "glob": "^8.0.3",
    "gunzip-maybe": "^1.4.2",
    "html-webpack-plugin": "^5.5.0",
    "inquirer": "^9.1.4",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "intersection-observer": "^0.12.2",
    "jsesc": "^3.0.2",
    "match-sorter": "^6.3.1",
    "minimist": "^1.2.7",
    "mutation-observer": "^1.0.3",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "portfinder": "^1.0.32",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.0.2",
    "pretty-error": "^4.0.0",
    "progress": "^2.0.3",
    "prop-types": "^15.8.1",
    "raf": "^3.4.1",
    "raw-loader": "^4.0.2",
    "react-helmet": "^6.1.0",
    "react-universal-component": "^4.5.0",
    "resolve-from": "^5.0.0",
    "serve": "^14.1.2",
    "shorthash": "^0.0.2",
    "slash": "^5.0.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "style-loader": "3.3.1",
    "swimmer": "^1.4.0",
    "tar-fs": "^2.1.1",
    "terser-webpack-plugin": "^5.3.6",
    "upath": "^2.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-flush-chunks": "^2.0.3",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-helmet": "^6.1.6",
    "babel-jest": "^29.3.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "enzyme-to-json": "^3.6.2",
    "express": "^4.18.2",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "onchange": "^7.1.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.1",
    "rimraf": "^3.0.2",
    "webpack-hot-middleware": "^2.25.3"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testRegex": "(/__tests__/.*\\.(test))\\.jsx?$",
    "moduleNameMapper": {
      "static.config.js$": "<rootDir>/src/static/__mocks__/static.config.js.mock.js"
    },
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "gitHead": "875004e6e75d36043382b8ec8bba27a766c1a83a"
}
