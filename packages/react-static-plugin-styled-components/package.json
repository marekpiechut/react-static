{
  "name": "react-static-plugin-styled-components",
  "version": "7.6.2",
  "description": "A React-Static plugin that adds CSS-in-JS/SSR support for styled-components",
  "main": "node.api.js",
  "repository": "https://github.com/react-static/react-static.git",
  "author": "Tanner Linsley <tannerlinsley@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir .",
    "watch": "yarn build --watch",
    "test": "yarn format",
    "format": "prettier src/**/*.js --write",
    "preversion": "yarn build"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "styled-components": ">=4"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "react": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "gitHead": "875004e6e75d36043382b8ec8bba27a766c1a83a"
}
